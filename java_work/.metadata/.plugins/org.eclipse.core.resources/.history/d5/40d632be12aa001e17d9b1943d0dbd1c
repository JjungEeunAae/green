package ex01;

public class Programmers {
	public static void main(String[] args) {
		int[] arr = {1, 2, 3, 4, 5};
		System.out.println(solution(arr));
	}
	public static int[] solution(int[] numbers) {
        int[] answer = {};
        for(int i = 0 ; i < numbers.length ; i++) {
        	answer[i] += numbers[i] * 2;
        }
        return answer;
    }
}
