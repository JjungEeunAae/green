package ex03;

public class Ex08 {
	public static void main(String[] args) {
		/* int -> char
		 * unicode A => 65
		 * 65 => A
		 * 실수 -> 정수
		 * double -> int : 소수점을 버리고 형변환된다.
		 */
		double d = 85.4d;
		int score = (int)d;
		System.out.println("score = " + score);
		System.out.println("d = " + d); 
		
		 /*
		 * 정수 -> 실수
		 * 10 -> 10.0
		 * 20 -> 20.0
		 */
		 int aa = 10;
		 double ee = aa;
		 System.out.println("aa = " + aa);
		 System.out.println("ee = " + ee);
	}
}
