package ex03;

import java.util.Scanner;

public class Ex03 {
	public static void main(String[] args) {
//		String a = "abcd";
//		System.out.println(a.toUpperCase());
		
		Scanner sc = new Scanner(System.in);
        String a = sc.next();
        char b;
        
        for(int i = 0 ; i < a.length() ; i++) {
        	b = a.charAt(i);
        	System.out.println(b);
//        	if(b == )
        }
	}
}
