package ex01;

public class Programmers {
	public static void main(String[] args) {
		int[] arr = {9, -1, 0};
		System.out.println(solution(arr));
	}
	public static int solution(int[] array) {
        int answer = array.length % 2 + 1;
        int result = array[answer];
        return result;
    }
}
