package ex03_1;

public class Ex01 {
	public static void main(String[] args) {
		// 나눈 후 나머지 몫 : 1
		System.out.println(3%2);
		// 2 -> 2진수 -> 0010
		// (<<)비트를 왼쪽으로 2회 이동 -> 1000 -> 8
		System.out.println(2<<2);
		// 비교연산자
		System.out.println(10>3);
		System.out.println(10<=3);
		System.out.println(10==10);
		System.out.println(10!=10);
		// 비교연산자 실습
		int a = 10;
		int b = 20;
		System.out.println(a==b);
		System.out.println(a!=b);
	}
}
