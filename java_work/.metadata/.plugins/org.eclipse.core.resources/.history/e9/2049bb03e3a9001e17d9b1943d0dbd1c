package ex03;

import java.util.Scanner;

public class Ex10 {
	public static void main(String[] args) {
		/*
		 * 실수는 소수 두번째
		 * 숫자를 입력받아 소수 둘째자리까지만 출력해라
		 * 
		 * ex) 23.456 -> 23.45
		 * 	   11.132 -> 11.13
		 */
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("숫자 입력");
		
		// next         -> 빈공백전까지 문자열 입력
		// nextLine()   -> 문자열 한줄 입력
		// nextInt()    -> 정수입력
		// nextDouble() -> 실수 입력
		double d = sc.nextDouble();
	}
}
